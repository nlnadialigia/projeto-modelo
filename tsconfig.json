{
  "compilerOptions": {
    "outDir": "./lib",
    "types": [
      "node",
      "jest"
    ],
    "target": "es2021",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "exclude": [
      "node_modules",
      "lib"
    ],
    "resolveJsonModule": true,
    "moduleResolution": "node"
  }
}